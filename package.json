{
  "name": "Youtube-clone-react",
  "version": "1.0.0",
  "description": "Movies search application",
  "main": "index.js",
  "repository": "git@github.com:VladTarnovskiy/Youtube-clone-react.git",
  "author": "vladtarnovskiy <vladtarnovski989@gmail.com>",
  "license": "MIT",
  "type": "module",
  "config": {
    "dev": "--config __webpack__/dev.config.js",
    "prod": "--config __webpack__/prod.config.js"
  },
  "scripts": {
    "dev": "nodemon --watch \"./__webpack__/*\" --exec npm run start-dev",
    "prod": "nodemon --watch \"./__webpack__/*\" --exec npm run start-prod",
    "start-dev": "cross-env-shell webpack serve ${npm_package_config_dev}",
    "start-prod": "cross-env-shell webpack serve ${npm_package_config_prod}",
    "build-dev": "cross-env-shell webpack ${npm_package_config_dev}",
    "build-prod": "cross-env-shell webpack ${npm_package_config_prod} --stats-children",
    "clean": "rd /s /q build",
    "lint": "eslint src --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "format:fix": "prettier --write \"./src/**/*.+(js|tsx|ts)\"",
    "test": "cross-env jest --config __jest__/jest.config.js",
    "test-watch": "jest --watch --config __jest__/jest.config.js",
    "test-coverage": "jest --coverage --config __jest__/jest.config.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/redux-logger": "^3.0.12",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "favicons-webpack-plugin": "^6.0.1",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.0",
    "image-minimizer-webpack-plugin": "^3.8.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "jest": "^29.7.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.2",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.3.0",
    "prettier": "3.1.1",
    "react-refresh": "^0.14.0",
    "redux-logger": "^3.0.6",
    "sass-loader": "^13.3.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@types/styled-components": "^5.1.34",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "styled-components": "^6.1.3"
  }
}
